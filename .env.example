# Environment Variables

# Private key for deployment (NEVER commit this!)
PRIVATE_KEY=your_private_key_here

# RPC URLs
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Basescan API Key for contract verification
BASESCAN_API_KEY=your_basescan_api_key_here

# Next.js
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Contract Addresses (update after deployment)
NEXT_PUBLIC_BTAP_TOKEN_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_BASETAPPER_NFT_ADDRESS=0x0000000000000000000000000000000000000000

# Chain ID (8453 for Base mainnet, 84532 for Base Sepolia)
NEXT_PUBLIC_CHAIN_ID=84532
